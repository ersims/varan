  "homepage": "https://github.com/ersims/varan#README.md",
  "engines": {
    "node": ">= 8.11.3",
    "npm": ">= 5.6.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
