    "collectCoverageFrom": [
      "webpack/**/*.{ts,js}?(x)"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|ts)x?$": "<rootDir>/test/fixtures/jestPreprocessor.js"
    }
