{
  "name": "varan-example-basic-static",
  "description": "This is a varan example project showcasing how to do a basic static app",
  "author": "ersims",
  "homepage": "https://github.com/ersims/varan#README.md",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">= 8.11.3",
    "npm": ">= 5.6.0"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie < 11"
  ],
  "scripts": {
    "build": "varan build ./webpack.config.js",
    "build:analyze": "varan build ./webpack.config.js --analyze",
    "clean": "del-cli ./dist",
    "test": "jest",
    "prewatch": "npm run clean",
    "start:watch": "varan watch ./webpack.config.js -- --inspect"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "core-js-pure": "^3.2.1",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.10"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "del-cli": "^2.0.0",
    "jest": "^24.8.0",
    "normalize.css": "^8.0.1",
    "react-test-renderer": "^16.8.6",
    "html-webpack-plugin": "^3.2.0",
    "varan": "*",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}?(x)",
      "*.js"
    ],
    "coverageDirectory": ".coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/__tests__/**/*.{ts,js}?(x)"
    ],
    "transform": {
      "^.+\\.(js|ts)x?$": "<rootDir>/test/fixtures/jestPreprocessor.js"
    },
    "silent": true,
    "verbose": true
  }
}
