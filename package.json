{
  "name": "varan",
  "version": "0.12.0",
  "description": "A webpack starter kit for offline-first bring-your-own-code apps with server side rendering",
  "author": "ersims",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersims/varan.git"
  },
  "keywords": [
    "varan",
    "create-react-app",
    "react",
    "webpack",
    "ssr",
    "pwa",
    "offline",
    "serviceworker",
    "boilerplate"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/ersims/varan/issues"
  },
  "homepage": "https://github.com/ersims/varan#README.md",
  "engines": {
    "node": ">= 8.11.3",
    "npm": ">= 5.6.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "varan": "./dist/bin/cli.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "apply:format": "prettier --write \"**/*.{css,less,sass,scss,html,md,graphql,gql,json,ts,tsx,js,jsx,yml,yaml}\"",
    "prebuild": "npm run clean",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && prettier --write \"CHANGELOG.md\"",
    "check:format": "prettier --list-different \"**/*.{css,less,sass,scss,html,md,graphql,gql,json,ts,tsx,js,jsx,yml,yaml}\"",
    "check:linting": "eslint --cache --cache-location node_modules/.cache/.eslintcache \"*.{ts,js}?(x)\" \"{babel,scripts,src,template,test,types,webpack}/**/*.{ts,js}?(x)\"",
    "check:types": "tsc --noEmit",
    "check": "npm-run-all check:*",
    "clean": "del-cli ./.coverage ./dist",
    "prepare": "npm run build",
    "release": "echo Recommended semver version bump: && conventional-recommended-bump -p angular && np",
    "pretest": "npm run check",
    "test": "jest",
    "version": "node scripts/bumpTemplateVersion && prettier --write \"template/package.json\" && git add \"template/package.json\" && npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.5.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/plugin-transform-react-jsx-self": "^7.0.0",
    "@babel/plugin-transform-react-jsx-source": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@samverschueren/stream-to-observable": "^0.3.0",
    "@svgr/webpack": "^4.3.1",
    "@types/wait-on": "^3.2.0",
    "@types/webpack": "^4.4.34",
    "@types/webpack-serve": "^2.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.2",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "compression-webpack-plugin": "^3.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.0",
    "dart-sass": "^1.22.4",
    "detect-port-alt": "^1.1.6",
    "execa": "^2.0.0",
    "fibers": "^4.0.1",
    "file-loader": "^4.0.0",
    "filesize": "^4.0.0",
    "fs-extra": "^8.1.0",
    "http-proxy-middleware": "^0.19.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "inquirer": "^6.5.0",
    "koa-connect": "^2.0.1",
    "listr": "^0.14.2",
    "lodash": "^4.17.14",
    "mini-css-extract-plugin": "^0.8.0",
    "ora": "^3.4.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "resolve-url-loader": "^3.1.0",
    "responsive-loader": "^1.2.0",
    "rxjs": "^6.5.2",
    "sass-loader": "^7.1.0",
    "sharp": "^0.23.0",
    "source-map-support": "^0.5.12",
    "split": "^1.0.1",
    "style-loader": "^0.23.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "table": "^5.4.1",
    "terser-webpack-plugin": "^1.3.0",
    "update-notifier": "^3.0.1",
    "url-loader": "^2.0.1",
    "validate-npm-package-name": "^3.0.0",
    "wait-on": "^3.3.0",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-hot-client": "^4.1.2",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-pwa-manifest": "^4.0.0",
    "webpack-serve": "^2.0.2",
    "webpack-serve-overlay": "^0.3.0",
    "webpack-serve-waitpage": "^1.0.2",
    "webpack-stats-plugin": "^0.3.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.5.4",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/execa": "^0.9.0",
    "@types/express": "^4.17.0",
    "@types/filesize": "^4.1.0",
    "@types/fs-extra": "^8.0.0",
    "@types/inquirer": "^6.0.3",
    "@types/jest": "^24.0.15",
    "@types/listr": "^0.14.0",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.2",
    "@types/ora": "^3.2.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/split": "^1.0.0",
    "@types/table": "^4.0.6",
    "@types/update-notifier": "^2.5.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-core": "^7.0.0-bridge.0",
    "conventional-changelog-cli": "^2.0.21",
    "conventional-recommended-bump": "^5.0.0",
    "del-cli": "^2.0.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "express": "^4.17.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "memory-fs": "^0.4.1",
    "normalize.css": "^8.0.1",
    "np": "^5.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.6",
    "react-test-renderer": "^16.8.6",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.4.2",
    "core-js": "^2.0.0 || ^3.0.0"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}?(x)",
      "webpack/**/*.{ts,js}?(x)",
      "*.js"
    ],
    "coverageDirectory": ".coverage",
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^fibers$": "<rootDir>/test/__mocks__/fibers.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|sass|scss|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/template/test/__mocks__"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/__tests__/**/*.{ts,js}?(x)",
      "<rootDir>/template/test/__tests__/**/*.{ts,js}?(x)"
    ],
    "transform": {
      "^.+\\.(js|ts)x?$": "<rootDir>/test/fixtures/jestPreprocessor.js"
    },
    "silent": true,
    "verbose": true
  }
}
