{
  "name": "varan",
  "version": "0.2.7",
  "description": "A webpack starter kit for bring-your-own-code apps with server side rendering",
  "author": "ersims",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersims/varan.git"
  },
  "keywords": [
    "varan",
    "create-react-app",
    "react",
    "webpack",
    "ssr",
    "boilerplate"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/ersims/varan/issues"
  },
  "homepage": "https://github.com/ersims/varan#README.md",
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.6.0"
  },
  "main": "index.js",
  "bin": {
    "varan": "./cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "apply:format": "prettier --config .prettierrc --write \"{babel,examples,src,test,webpack}/**/*.{ts,js}?(x)\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "check:format": "prettier --config .prettierrc --list-different \"{babel,examples,src,test,webpack}/**/*.{ts,js}?(x)\"",
    "check": "npm-run-all check:*",
    "clean": "del-cli ./.coverage",
    "release": "echo Recommended semver version bump: && conventional-recommended-bump -p angular && np",
    "pretest": "npm run check",
    "test": "jest",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.51",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-jsx-self": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-jsx-source": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-typescript": "^7.0.0-beta.51",
    "babel-loader": "^8.0.0-beta.4",
    "commander": "^2.15.0",
    "compression-webpack-plugin": "^1.1.11",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.0",
    "detect-port-alt": "^1.1.6",
    "file-loader": "^1.1.11",
    "http-proxy-middleware": "^0.18.0",
    "koa-connect": "^2.0.1",
    "lodash.castarray": "^4.4.0",
    "lodash.defaults": "^4.2.0",
    "lodash.omit": "^4.5.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "postcss-preset-env": "^5.1.0",
    "react-dev-utils": "^5.0.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.3",
    "shelljs": "^0.8.2",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "update-notifier": "^2.5.0",
    "url-loader": "^1.0.1",
    "validate-npm-package-name": "^3.0.0",
    "webpack": "^4.12.0",
    "webpack-hot-client": "^4.0.3",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2",
    "webpack-pwa-manifest": "^3.6.2",
    "webpack-serve": "^2.0.0",
    "webpack-stats-plugin": "^0.2.1"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.4.0",
    "conventional-changelog-cli": "2.0.1",
    "conventional-recommended-bump": "4.0.0",
    "del-cli": "1.1.0",
    "express": "4.16.3",
    "husky": "1.0.0-rc.13",
    "jest": "23.4.0",
    "memory-fs": "0.4.1",
    "normalize.css": "8.0.0",
    "np": "3.0.4",
    "npm-run-all": "4.1.3",
    "prettier": "1.13.7",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.3",
    "react-test-renderer": "16.4.1"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}?(x)",
      "webpack/**/*.{ts,js}?(x)",
      "*.js"
    ],
    "coverageDirectory": ".coverage",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "moduleNameMapper": {
      "./clearConsole": "<rootDir>/test/__mocks__/react-dev-utils/clearConsole.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|sass|scss|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/examples/(.*)/test/__mocks__"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/__tests__/**/*.{ts,js}?(x)",
      "<rootDir>/examples/**/test/__tests__/**/*.{ts,js}?(x)"
    ],
    "transform": {
      "^.+\\.(jsx?|tsx?)$": "<rootDir>/test/fixtures/jestPreprocessor.js"
    },
    "silent": true,
    "verbose": true
  }
}
