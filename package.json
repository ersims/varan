{
  "name": "varan",
  "version": "0.0.4",
  "description": "A webpack starter kit for bring-your-own-code apps with server side rendering",
  "author": "ersims",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersims/varan.git"
  },
  "keywords": [
    "varan",
    "create-react-app",
    "react",
    "webpack",
    "ssr",
    "boilerplate"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/ersims/varan/issues"
  },
  "homepage": "https://github.com/ersims/varan#README.md",
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.6.0"
  },
  "main": "index.js",
  "bin": {
    "varan": "./cli.js"
  },
  "scripts": {
    "clean": "del-cli ./.tmp_test_artifacts ./.coverage",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.44",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx-self": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx-source": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "babel-loader": "^8.0.0-beta.2",
    "commander": "^2.15.0",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.11",
    "detect-port-alt": "^1.1.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "http-proxy-middleware": "^0.18.0",
    "koa-connect": "^2.0.1",
    "lodash.defaults": "^4.2.0",
    "lodash.omit": "^4.5.0",
    "node-sass": "^4.8.3",
    "react-dev-utils": "^5.0.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "shelljs": "^0.8.1",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "validate-npm-package-name": "^3.0.0",
    "webpack": "^4.4.1",
    "webpack-manifest-plugin": "^2.0.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-serve": "^0.3.1",
    "webpack-stats-plugin": "^0.2.1"
  },
  "devDependencies": {
    "del-cli": "^1.1.0",
    "express": "^4.16.3",
    "jest": "^22.4.3",
    "memory-fs": "^0.4.1",
    "normalize.css": "^8.0.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.0"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "webpack/**/*.js",
      "index.js",
      "cli.js",
      "examples/**/src/**/*.js",
      "examples/**/webpack/**/*.js"
    ],
    "coverageDirectory": ".coverage",
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "testMatch": [
      "**/test/tests/**/*.js?(x)"
    ],
    "roots": [
      "<rootDir>/test"
    ],
    "moduleNameMapper": {
      "./clearConsole": "<rootDir>/test/__mocks__/react-dev-utils/clearConsole.js"
    },
    "projects": [
      "<rootDir>",
      "<rootDir>/examples/*"
    ],
    "testEnvironment": "node",
    "silent": true,
    "verbose": true
  }
}
